      "name": "New Appointment",
      "short_name": "Appointment",
      "description": "Schedule a new appointment",
      "url": "/appointments/new",
      "icons": [
        {
          "src": "/icons/calendar-96x96.png",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "Client Portal",
      "short_name": "Clients",
